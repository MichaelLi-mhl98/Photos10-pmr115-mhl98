				<TextField fx:id="numPhotos" editable="false" layoutX="320.0" layoutY="269.0" prefHeight="25.0" prefWidth="240.0" promptText="Number of Photos" />
				<TextField fx:id="dateRange" editable="false" layoutX="320.0" layoutY="294.0" prefHeight="25.0" prefWidth="240.0" promptText="Date Range:" />
				<Button fx:id="delete" onAction="#deleteAlbum" layoutX="320.0" layoutY="70.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="110.0" text="Delete Album">
					<font>
						<Font name="Comic Sans MS" size="12.0" />
					</font>
				</Button>
				<Button fx:id="edit" onAction="#editAlbum" layoutX="320.0" layoutY="108.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="110.0" text="Rename Album">
